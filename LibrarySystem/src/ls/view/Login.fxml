<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.*?>

<AnchorPane fx:id="rootLayout" prefHeight="186.0" prefWidth="459.0"
	styleClass="border-pane" stylesheets="@resources/application.css"
	xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="ls.controller.LoginController">
	<children>
		<Label fx:id="lblMessage" layoutX="72.0" layoutY="10.0"
			styleClass="error" text="" />
		<GridPane hgap="10.0" layoutX="72.0" layoutY="35.0"
			prefHeight="131.0" prefWidth="315.0" vgap="10.0">
			<columnConstraints>
				<ColumnConstraints hgrow="SOMETIMES" minWidth="10.0"
					prefWidth="100.0" />
				<ColumnConstraints hgrow="SOMETIMES" minWidth="10.0"
					prefWidth="100.0" />
			</columnConstraints>
			<rowConstraints>
				<RowConstraints minHeight="10.0" prefHeight="30.0"
					vgrow="SOMETIMES" />
				<RowConstraints maxHeight="44.0" minHeight="10.0"
					prefHeight="39.0" vgrow="SOMETIMES" />
				<RowConstraints maxHeight="20.0" minHeight="6.0"
					prefHeight="11.0" vgrow="SOMETIMES" />
				<RowConstraints minHeight="10.0" prefHeight="30.0"
					vgrow="SOMETIMES" />
			</rowConstraints>
			<children>
				<Label text="User ID" />
				<TextField fx:id="txtUserId" prefWidth="124.0"
					GridPane.columnIndex="1" />
				<Label text="Password" GridPane.rowIndex="1" />
				<PasswordField fx:id="txtPassword" prefHeight="22.0"
					prefWidth="124.0" GridPane.columnIndex="1" GridPane.rowIndex="1" />
				<Button mnemonicParsing="false" onAction="#btnLoginClick"
					text="Login" GridPane.columnIndex="1" GridPane.halignment="RIGHT"
					GridPane.rowIndex="3" />
			</children>
		</GridPane>

	</children>
</AnchorPane>
